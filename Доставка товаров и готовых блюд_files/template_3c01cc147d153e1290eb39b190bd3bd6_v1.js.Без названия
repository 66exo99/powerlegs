    })
        
    }
    
    
    if($('.dev-swiper').length){
    var swiper2 = new Swiper('.dev-swiper', {
      loop: true,
      spaceBetween: 0,
        navigation: {
            prevEl: '.dev-swiper-prev',
            nextEl: '.dev-swiper-next',     
      },
      
    });
        
 
      
       
    
   $('.dev-all-slides').text(swiper2.slides.length - 2);
    
    swiper2.on('slideChangeTransitionEnd', function () {
        $('.dev-current-slide').text(swiper2.realIndex+1);
        var title = $('.dev-swiper .swiper-slide-active').find('.dev-swiper__item').data('title');
        var price = $('.dev-swiper .swiper-slide-active').find('.dev-swiper__item').data('price-rub');
        var priceCop = $('.dev-swiper .swiper-slide-active').find('.dev-swiper__item').data('price-cop');
        var priceFor = $('.dev-swiper .swiper-slide-active').find('.dev-swiper__item').data('price-for');
        
        $('.dev-widget__title-js').text(title);
        $('.dev-widget__price-js').text(price);
        $('.dev-widget__priceCop-js').text(priceCop);
        $('.dev-widget__priceFor-js').text(priceFor);
        
    });
    }
    
    
    /*
    var aboutSliderNav = new Swiper('.aboutSliderNav', {
      
      slidesPerView: 5,
      spaceBetween: 0,
        
        navigation: {
            prevEl: '.dev-swiper-prev',
            nextEl: '.dev-swiper-next',     
      },
      
    });
    */ 
    
   if($('.swiper-news').length){ 
    var swiper3 = new Swiper('.swiper-news', {
       slidesPerView: 3,
      spaceBetween: 30,
     parallax:true,
         loop: true,
        
        navigation: {
            prevEl: '.swiper-news-prev',
            nextEl: '.swiper-news-next',     
      },
      breakpoints: {
        1400: {
          slidesPerView: 3,
          
        },
        1300: {
          slidesPerView: 2,
          
        },
         990: {
          slidesPerView: 2,
          
        },
      }
    });
    
    $('.swiper-news-count').text(swiper3.slides.length - 2);
   
    swiper3.on('slideChange', function (e) {
       
        $('.swiper-news-curent').text(swiper3.realIndex+1);
    })
     $('.swiper-news-count').text($('.swiper-news .swiper-slide:not(.swiper-slide-duplicate)').length);
   }
})


function reInitDatePicker()
{
    if($('.date-input').length){
        var tomorrow2;
        $('.date-input').each(function(indx, element){
          
            if ($(this).data("max")) {
                var maxDate=$(this).data("max");
                var maxDateAr=maxDate.split(".");
                tomorrow2=new Date(maxDateAr[2],maxDateAr[1]-1,maxDateAr[0]);
                
            }
            
            var InpVal = $(this).val();
            var dp = $(this).datepicker({autoClose:true}).data('datepicker');
            if(InpVal.length > 4){
               InpVal= InpVal.split('.');
               dp.selectDate(new Date(InpVal[2], InpVal[1]-1, InpVal[0]));
            }
            if($(this).data("days")){
              
                var days=$(this).data("days");
            }
            else{
                var days=0;
                if($('#cake-basket').length){
                    $('#cake-basket .cake-basket-section').each(function(){
                        if($(this).data("category")){
                            if($(this).data("category")!=33){
                                days=5;
                            }
                        }
                    })
                }
                
            }
            
            console.log(days);
            $(this).datepicker({
                 onRenderCell: function (date, cellType) {
                     if (cellType == 'day') {
                      

                        var now = new Date();
                         
                        var ms = now.getTime() + 86400000*days;
                       
                         
                         
                         
                        var day = date.getDay()

                        if(tomorrow2){        
                            if(now > date){
                                
                                return {
                                    disabled: true
                                   }
                            }
                            else if(tomorrow2 >= date){
                                return {
                                    disabled: false
                                }
                            }
                           
                            else{
                                return {
                                 disabled: true
                                }
                            }  
                        }
                        else{
                            var tomorrow = new Date(ms);
                            if(tomorrow > date){
                                return {
                                    disabled: true
                                }
                            }
                            else{
                                return {
                                 disabled: false
                                }
                            } 
                        }
                                          
                        
                    }
                },
                onSelect: function onSelect(fd, date) {
                    $('.contacts_tort_save').trigger("change");
                }

            })
            
        })
        
    }
}
/* End */
;
; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/main/js/custom.js?159886447021456";s:6:"source";s:34:"/local/templates/main/js/custom.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
//CUSTOM JS

// AJAX FORM
var options_ajax={
    beforeSubmit:  showRequest_ajax,  // pre-submit callback 
    success:       showResponse_ajax
};
function showResponse_ajax(responseText, statusText, xhr, $form){      
    $('#preloader_ajax').remove();
    console.log(responseText);
    var formElementId = $($form[0]).attr("id");
    var obj = jQuery.parseJSON(responseText);
    if(obj.status=="error"){
        var res_text="<span class='error'>"+obj.text+"</span>";
        $('#result_block').html(res_text)
    }
    else{
        $('#'+formElementId+" textarea,input[type='text']").val('');
        var res_text="<span class='success'>"+obj.text+"</span>";
        $('#result_block').html(res_text)
    }    
}
function showRequest_ajax(formData, jqForm, options){
    var formElementId = $(jqForm[0]).attr("id");
    var preloader="<img src='/local/templates/main/images/preloader.svg' id='preloader_ajax'>";
    $('#'+formElementId).find("input[type='submit'],button[type='submit']").after(preloader);
}
$(document).ready(function(){
    
    $('.check-mask').inputmask("9{1,3}.9{1,4}.9{1,4}");
    if($('.ajax-form').length){
        $('.ajax-form').ajaxForm(options_ajax);
    }    
})

//Журнал, фильтр
$(document).on("change","#journals_year",function(){
    location.href="/pokupatelyam/zhurnal-tabris/?SECTION_ID="+$(this).val();
})
//фильтрация вакансии
$(document).on("submit",".filter_vakansii_form",function(e){
    e.preventDefault();
    var that=$(this);
    that.closest(".tab").find(".tr_body").hide();
    
    var sferas=[];    
    $(this).find("input[name='sferas[]']").each(function(){
        if($(this).is(":checked")){
            sferas.push($(this).val());
        }
    })      

    var dates=[];    
    $(this).find("input[name='dates[]']").each(function(){
        if($(this).is(":checked")){
            dates.push($(this).val());
        }
    })
    if(dates.length>0){
        $.each(dates,function(index,item){
            var lastDate = new Date();
            lastDate.setDate(lastDate.getDate() - item);
            that.closest(".tab").find(".tr_body").each(function(){
            let date = new Date($(this).data("date"));
            let sfera = $(this).data("sfera");
            if(lastDate<=date){
                if(sferas.indexOf(sfera)===-1&&sferas.length>0){
                    $(this).hide();                    
                }
                else{
                    $(this).css("display","table-row");
                }
            }   
            else{
                $(this).hide();
            }
                
            })
        });
    }
    else{
        if(sferas.length>0){
            $.each(sferas,function(index,item){
                that.closest(".tab").find(".tr_body[data-sfera='"+item+"']").css("display","table-row");
            })
        }
        else{           
            that.closest(".tab").find(".tr_body").css("display","table-row");
           
        }      
    }

})
//фильтр вакансий
$(document).on("click",".filter_vakansii_form .filter-reset",function(){
    setTimeout(function(){
    $(".filter_vakansii_form").submit();
    },100);
})

//попап вакансий аяксом
$(document).on("click",".vakancy-trigger",function(e){
    e.preventDefault();
    $('.ajax-vacancy').html("<img src='/local/templates/main/images/preloader.svg' id='preloader_ajax'>");
    var id=$(this).data("id");
    $.magnificPopup.open({
        items: {
            src: $('#vacancy-popup')
        },
        type: 'inline'
    });
    $.ajax({
        url:"/karera/vakansii/getVacancyPopup.php", 
       data:"id="+id, 
       success:function(result){      
           $('.ajax-vacancy').html(result);
       }
    });
})


