                                            break;
                                        case "extensions":
                                            n[r[i]] = s.attr(a).replace(/ /g, "").split(",");
                                            break;
                                        case "files":
                                            n[r[i]] = JSON.parse(s.attr(a));
                                            break;
                                        default:
                                            n[r[i]] = s.attr(a)
                                    }
