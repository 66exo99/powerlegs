$('.inline-popup-trigger').magnificPopup({
        mainClass:'mfp-zoom-out',     
        removalDelay: 300, //delay removal by X to allow out-animation      
          callbacks: {
            beforeOpen: function() {
                // just a hack that adds mfp-anim class to markup 
                if($(this.st.el).data("name")){
                    $('#korzina_zakaz').val($(this.st.el).data("name"));
                    if($('.location-select select').length){
                        let ar=[
                            "ул. Тургенева, д. 138/6",
                            "пр. Чекистов, 1/3",
                            "ул. Кубанская Набережная, д.25"
                        ];
                        let ar2=[                            
                            "пр. Чекистов, 1/3",
                            "ул. Кубанская Набережная, д.25"
                        ];
                        if($(this.st.el).data("name")=="Подарочная карта 10000 руб"){
                            $('.location-select select option').each(function(){
                                if(ar.indexOf($(this).val())===-1){
                                    $(this).prop('disabled',true);
                                }
                                else{
                                    $(this).prop('disabled',false);
                                }
                            })
                        }
                        else if($(this.st.el).data("name")=="Подарочная карта 15000"){
                            $('.location-select select option').each(function(){
                                if(ar2.indexOf($(this).val())===-1){
                                    $(this).prop('disabled',true);
                                }
                                else{
                                    $(this).prop('disabled',false);
                                }
                            })
                        }
                        else{
                            $('.location-select select option').each(function(){
                                $(this).prop('disabled',false);
                            });
                        }
                        $('.location-select select').select2({
                            width:'100%',
                            minimumResultsForSearch: Infinity
                        })

                        $('.location-select select option').not(":disabled").first().prop("selected",true).trigger('change');
                    }
                   
                }
               
                this.st.image.markup = this.st.image.markup.replace('mfp-figure', 'mfp-figure mfp-with-anim');               
            }
          },
})
