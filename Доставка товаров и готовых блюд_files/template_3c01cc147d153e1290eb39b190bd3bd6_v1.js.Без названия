 //категории табы
$(document).on("click",".tort-order-nav a",function(e){
    e.preventDefault();
    let href = $(this).attr("href");
    $('.tort-order-nav a').removeClass("active");
    $(this).addClass("active");
    $('.tort-tabs').removeClass("active");
    $(href).addClass("active");
})
//показаьт больше
$(document).on('click',".load_more_tort",function(e){
    e.preventDefault();
    var page=$(this).attr("data-page");
    var section=$(this).attr("data-section");
    var pages=$(this).data("pages");
    var preloader="<img src='/local/templates/main/images/preloader.svg' id='preloader_ajax'>";
    var that=$(this);
    $(this).after(preloader);
        $.ajax({
            url:location.href, 
           data:"IS_AJAX=Y&PAGEN_1="+page+"&ACTIVE_SECTION="+section, 
           success:function(result){          
               $('#preloader_ajax').remove();    
               $(".place_for_torts").append(result);
               page++;
               that.attr("data-page",page);
                $('.match-height').matchHeight();
               if(parseInt(page)>parseInt(pages)){
                   that.hide();
               }
           } 
        });
      
})

//ХОТИТЕ ИЗМЕНИТЬ ВЕС ТОРТА?
$(document).on("click",".want_to_change",function(e){
    e.preventDefault();
    $(this).hide();
    $('.custom_weight').show()
    $('.main_weight').hide();
});

$(document).on("click",".add2BasketBtn",function(e){
    e.preventDefault();
   
     var quant=$('#tort_count_basket').val();
    
   
    var id=$('#tort_count_basket').data('id');   
    var osnova="";
    if($('.tort-order-category__item_active').length){
        osnova=$('.tort-order-category__item_active').attr("data-name");
    }
    var weight=0;
    if($('#tort_weight').length){
        weight=parseFloat($('#tort_weight').val());
    }
    var tort_color="";
    if($('#tort_color').length){
        tort_color=($('#tort_color').val());
    }
    var tort_no_ingridient="";
    if($('#tort_no_ingridient').length){
        tort_no_ingridient=($('#tort_no_ingridient').val());
    }
    var tort_nadpis="";
    if($('#tort_nadpis').length){
        tort_nadpis=($('#tort_nadpis').val());
    }
    var yarus_count_hidden="";
    if($('#yarus_count_hidden').length){
        yarus_count_hidden=($('#yarus_count_hidden').val());
    }
    var fd = new FormData;
    fd.append('quantity', quant);
    fd.append('id', id);
    fd.append('osnova', osnova);
    fd.append('weight', weight);
    fd.append('tort_color', tort_color);
    fd.append('tort_no_ingridient', tort_no_ingridient);
    fd.append('tort_nadpis', tort_nadpis);
    fd.append('yarus_count_hidden', yarus_count_hidden);
   
  

    if($('#image').length) {
        
        fd.append('image', $('#image')[0].files[0]);
    }

    $.ajax({
        method:"POST",
        url:"/local/components/wf/zakaz_tort/add2Basket.php", 
        data:fd, 
        processData: false,
        contentType: false,
        success:function(result){        
            console.log(result); 
            Swal.fire({
                type: 'success',
                title: 'Добавлено',
                text: 'Товар добавлен к заказу',
                //timer: 1500                
               
              })
            $('#cake-basket').load(location.href+" #cake-basket >*",function(){
                reInitDatePicker();
            });
            $('.cake-ico__title').load(location.href+" .cake-ico__title");
        } 
    });      
})

$(document).on("click",".add2BasketBtn_vipechka",function(e){
    e.preventDefault();
   
     var quant=$('#tort_count_basket').val();
    
   
    var id=$('#tort_count_basket').data('id');   
    var osnova="";
    if($('.tort-order-category__item_active').length){
        osnova=$('.tort-order-category__item_active').attr("data-name");
    }
    var weight=0;
   
    var fd = new FormData;
    fd.append('quantity', quant);
    fd.append('id', id);

    if($('#image').length) {
        
        fd.append('image', $('#image')[0].files[0]);
    }

    $.ajax({
        method:"POST",
        url:"/local/components/wf/zakaz_vipechka/add2Basket.php", 
        data:fd, 
        processData: false,
        contentType: false,
        success:function(result){        
            console.log(result); 
            Swal.fire({
                type: 'success',
                title: 'Добавлено',
                text: 'Товар добавлен к заказу',
                //timer: 1500                
               
              })
            $('#cake-basket').load(location.href+" #cake-basket >*",function(){
                reInitDatePicker();
            });
            $('.cake-ico__title').load(location.href+" .cake-ico__title");
        } 
    });      
})
//количество позиций
$(document).on('change',"#tort_count_basket",function(e){
    e.preventDefault();
    var quant=$(this).val();
    var min=$(this).attr("data-min");
    if(quant<min){
        $(this).val(min);
    }
    if($('#local_price_1').length){
        let base=parseFloat($('#local_price_1').data("base"));
       
        let weight=parseFloat($('#tort_weight').val());
       
        let local_price_1=Math.ceil(base*$(this).val()*weight);
        $('#local_price_1').text(local_price_1);
    }  
     if($('#local_price_1_vipechka').length){
         console.log('vipechka_price_recalc');
        let base=parseFloat($('#local_price_1_vipechka').data("base"));
       
        let local_price_1=Math.ceil(base*$(this).val());
        $('#local_price_1_vipechka').text(local_price_1);
    }
})
//вес позиций
$(document).on('change',"#tort_weight",function(e){
    e.preventDefault();
    var weight=parseFloat($(this).val());
    var min=parseFloat($(this).attr("data-min"));
    var max=parseFloat($(this).attr("data-max"));
    if(weight<min){
        weight=min;
        $(this).val(min);
    }
    if(weight>max){
        weight=max;
        $(this).val(max);
    }
    
    if($('#local_price_1').length){
      
        let base=parseFloat($('#local_price_1').data("base"));
        let quant=parseInt($('#tort_count_basket').val());
        let local_price_1=Math.ceil(base*quant*weight);
        
        $('#local_price_1').text(local_price_1);
    }    
    $('#weight_span').text(weight);
    changeYarusCount(weight);

    if ($('.standart_date').length) {
        if(weight!=min) {
           
            $('.date-input').data("days",1);
        }
        else{
            $('.date-input').data("days",0);
        }
        reInitDatePicker();
    }
})

//
$(document).on("change","#tort_count_basket_popup",function(e){
    var quant=$(this).val();
    var min=$(this).attr("data-min");
    if(quant<min){
        $(this).val(min);
    }
    var quant=$(this).val();
    var id=$(this).data('id');   

   
    var osnova="";
    if($('.tort-order-category__item_active').length){
        osnova=$('.tort-order-category__item_active').attr("data-name");
    }
    var weight=0;
    if($('#tort_weight').length){
        weight=parseFloat($('#tort_weight').val());
    }
    var tort_color="";
    if($('#tort_color').length){
        tort_color=($('#tort_color').val());
    }
    var tort_no_ingridient="";
    if($('#tort_no_ingridient').length){
        tort_no_ingridient=($('#tort_no_ingridient').val());
    }
    var tort_nadpis="";
    if($('#tort_nadpis').length){
        tort_nadpis=($('#tort_nadpis').val());
    }

    $('.cake-basket-list').css("opacity",0.6);
    $.ajax({
        method:"POST",
        url:"/local/components/wf/zakaz_tort/add2Basket.php", 
        data:"quantity="+quant+"&id="+id+"&osnova="+osnova+"&weight="+weight+"&tort_color="+tort_color+"&tort_no_ingridient="+tort_no_ingridient+"&tort_nadpis="+tort_nadpis+"&type=clicker", 
        success:function(result){     
            
            $('#cake-basket').load(location.href+" #cake-basket >*",function(){
                $('.cake-basket-list').css("opacity",1);
                $('#cake-basket').append('<button title="Close (Esc)" type="button" class="mfp-close">×</button>');
            });
            $('.cake-ico__title').load(location.href+" .cake-ico__title");
        
        } 
    });
})

//удалить торт из корзины
$(document).on("click",".delete_tort_from_basket",function(e){
    e.preventDefault();
    var id=$(this).data('id');
    var redirect=$(this).data('redirect');   
    $.ajax({
        method:"POST",
        url:"/local/components/wf/zakaz_tort/removeFromBasket.php", 
        data:"id="+id, 
        success:function(result){          
            if(redirect){
                location.href="/pokupatelyam/zakaz-tortov/forma-zakaza/";
            }
            else{
                $('.cake-add').load(location.href+" #cake-basket >*",function(){
                    reInitDatePicker();
                });
                $('.cake-ico__title').load(location.href+" .cake-ico__title");
            }
           
        } 
    });      
})
//отменяем заказа
$(document).on("click",".delete_full_order",function(e){
    e.preventDefault();
    $.ajax({
        method:"POST",
        url:"/local/components/wf/zakaz_tort/cancelOrder.php",       
        success:function(result){ 
            location.href="/pokupatelyam/zakaz-tortov/forma-zakaza/";    
        } 
    });      
   
})

