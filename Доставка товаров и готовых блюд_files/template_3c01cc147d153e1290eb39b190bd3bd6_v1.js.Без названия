$(document).ready(function(){
    jQuery.fn.ForceNumericOnly =
function()
{
    return this.each(function()
    {
        $(this).keydown(function(e)
        {
            var key = e.charCode || e.keyCode || 0;
            // allow backspace, tab, delete, enter, arrows, numbers and keypad numbers ONLY
            // home, end, period, and numpad decimal
            return (
                key == 8 || 
                key == 9 ||
                key == 13 ||
                key == 46 ||
                key == 110 ||
                key == 190 ||
                (key >= 35 && key <= 40) ||
                (key >= 48 && key <= 57) ||
                (key >= 96 && key <= 105));
        });
    });
};

    $('body').on('click', '.save-and-add', function (e){
        e.preventDefault();
        $.magnificPopup.close();
       
            $.magnificPopup.open({
                items: {
                    src: '#add-cake' 
                },
                type: 'inline'
            });
        
        
    });
    
$('.counter__input').ForceNumericOnly();
    $('body').on('click', '.counter__control_plus', function (){
        var step=parseInt($(this).data('step'));
        if(!step){
            step=1;
        }
       
        var val =  $(this).closest('.counter').find('.counter__input').val()*1;
        var min =  $(this).closest('.counter').find('.counter__input').attr("data-min");
        var max =  $(this).closest('.counter').find('.counter__input').attr("data-max");
        $(this).closest('.counter').find('.counter__input').val(val+step);
        $(this).closest('.counter').find('.counter__input').trigger("change");
        if(step==500){
            if(val+step<=max){
                $('#weight_span').text(val+step);
            }
           
        }
    });
    $('body').on('click', '.counter__control_minus', function (){
        var step=parseInt($(this).data('step'));
        if(!step){
            step=1;
        }
        var val =  $(this).closest('.counter').find('.counter__input').val()*1;
        if(val == 0){
            val = 0;
        }
        else{
            val-=step;
        }
        if(val<parseInt($(this).closest('.counter').find('.counter__input').data("min"))){
            val=parseInt($(this).closest('.counter').find('.counter__input').data("min"));
        }
        if(step==500){
            $('#weight_span').text(val);
        }
        $(this).closest('.counter').find('.counter__input').val(val);
        $(this).closest('.counter').find('.counter__input').trigger("change");
    })
    
    
