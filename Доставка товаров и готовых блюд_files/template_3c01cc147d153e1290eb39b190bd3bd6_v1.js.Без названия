//сохраняем контактную инфу в сессию
$(document).on("change",".contacts_tort_save",function(){
    var data=$('.cake-form').serialize();
    $.ajax({
        method:"POST",
        url:"/local/components/wf/zakaz_tort/setContacts.php", 
        data:data, 
        success:function(result){          
            $('#cake-basket').load(location.href+" #cake-basket >*");
        } 
    });      
})

//перед открытием попапа с оформлением заказа проверим что заполнены поля
$(document).on("click",".open_basket_link",function(e){
    e.preventDefault();
    var error=false;
    $('.contacts_tort_save').each(function(){
        if(!$(this).val()||$(this).val().trim()==''){
            error=true;
            $(this).parent().addClass("error");
        }
        else{
            $(this).parent().removeClass("error");
        }
    })
    if(!error){
        if($('#agree_zakaz').is(":checked")){
            $.magnificPopup.open({
                items: {
                    src: '#cake-basket' 
                },
                type: 'inline'
            });
        }
        else{
            Swal.fire({
                type: 'warning',
                title: 'Ошибка',
                text: 'Пожалуйста, подвердите согласие на обработку персональных данных',
               
              })
        }
       
    }
})

//оформление заказа
$(document).on("click",".make_order_ajax",function(e){
    e.preventDefault();
    var that=$(this);
    if(!that.attr("disabled")){        
        //$.magnificPopup.close();
        var error=false;
        $('.final_tort_hidden').each(function(){
            if(!$(this).val()){
                error=true;
            }
        })
       
        if(!error){
            that.attr("disabled",true);
            that.css("opacity",0.4);
            $.ajax({
                method:"POST",
                url:"/local/components/wf/zakaz_tort/makeOrder.php", 
                data:$('.cake-form').serialize(), 
                dataType: "json",
                success:function(result){    
                    that.attr("disabled",false);    
                    that.css("opacity",1); 
                    if(result.status=="ok"){
                        $('.cake-ico__title').load(location.href+" .cake-ico__title");
                        $('#result-block').html("<div style='font-size:18px'><strong>Ваш заказ оформлен</strong></div>");
                    }
                    else{
                        Swal.fire({
                            type: 'warning',
                            title: 'Ошибка',
                            text: result.text,
                        
                        })
                    }
                   
                } 
            });      
        
            
        }
        else{
            Swal.fire({
                type: 'warning',
                title: 'Ошибка',
                text: 'Пожалуйста, заполните контактные данные',
            
            })
        }
    }
})

$(document).on("click",".make_order_ajax_vipechka",function(e){
    e.preventDefault();
    var that=$(this);
    if(!that.attr("disabled")){        
        //$.magnificPopup.close();
        var error=false;
        $('.final_tort_hidden').each(function(){
            if(!$(this).val()){
                error=true;
            }
        })
       
        if(!error){
            that.attr("disabled",true);
            that.css("opacity",0.4);
            $.ajax({
                method:"POST",
                url:"/local/components/wf/zakaz_vipechka/makeOrder.php", 
                data:$('.cake-form').serialize(), 
                dataType: "json",
                success:function(result){    
                    that.attr("disabled",false);    
                    that.css("opacity",1); 
                    if(result.status=="ok"){
                        $('.cake-ico__title').load(location.href+" .cake-ico__title");
                        $('#result-block').html("<div style='font-size:18px'><strong>Ваш заказ оформлен</strong></div>");
                    }
                    else{
                        Swal.fire({
                            type: 'warning',
                            title: 'Ошибка',
                            text: result.text,
                        
                        })
                    }
                   
                } 
            });      
        
            
        }
        else{
            Swal.fire({
                type: 'warning',
                title: 'Ошибка',
                text: 'Пожалуйста, заполните контактные данные',
            
            })
        }
    }
})

//проверка регистрации галочки
$(document).on("submit","#regForm",function(e){
  
    if($('#regForm .checkbox__input').length){
        if(!$('#regForm .checkbox__input').is(":checked")){
         alert('Пожалуйста, подвердите согласие с пользовательским соглашением');
            return false;
        }
    }
})

$(document).on("change","#reg_check_checkbox",function(){
    if($(this).is(":checked")){
        $('#reg_submit_btn').prop("disabled",false);
    }
    else{
        $('#reg_submit_btn').prop("disabled",true);
    }
})
$(document).on("click",".closeMagnif",function(e){
    e.preventDefault();
    $.magnificPopup.close();
})


function changeYarusCount(weight)
{   
    if(typeof weights !== "undefined") {
        $.each(weights,function(key,value){       
            if(parseInt(value.PROPERTY_WEIGHT_FROM_VALUE)<=parseInt(weight)&&parseInt(value.PROPERTY_WEIGHT_TO_VALUE)>=parseInt(weight)){            
                $('#yarus_count').text(value.PROPERTY_YARUS_VALUE+" "+declOfNum(value.PROPERTY_YARUS_VALUE,['ярус','яруса','ярусов']));
                $('#yarus_count_hidden').val(value.PROPERTY_YARUS_VALUE);
            }        
        })
    }
 
}

function declOfNum(number, titles) {  
    cases = [2, 0, 1, 1, 1, 2];  
    return titles[ (number%100>4 && number%100<20)? 2 : cases[(number%10<5)?number%10:5] ];  
}


$('.toggler-trigger').click(function (e) {
  e.preventDefault();
  $(this).toggleClass('active');
  $(this).closest('.toggler').find('.toggler__content').stop().slideToggle();
  $(this).closest('.toggler').toggleClass('active');
});

$(document).ready(function(){
    $('.single-new__content iframe').wrap('<div class="iframe-wrapper">');
    
    $('.select_js').select2({
        width: '100%',
        height: '100%',
        minimumResultsForSearch: -1
    });

});


var Detect = {
    init: function () {
        this.browser = this.searchString(this.dataBrowser);
    },
    searchString: function (data) {
        for (var i=0;i<data.length;i++) {
            var dataString = data[i].string;
            var dataProp = data[i].prop;
            this.versionSearchString = data[i].versionSearch || data[i].identity;
            if (dataString) {
                if (dataString.indexOf(data[i].subString) != -1)
                    return data[i].identity;
            }
            else if (dataProp)
                return data[i].identity;
        }
    },
    dataBrowser: [
        {
            string: navigator.userAgent,
            subString: "MSIE",
            identity: "IE",
            versionSearch: "MSIE"
        }
    ]
    };
    Detect.init();
    
    if(Detect.browser == 'IE'){
       var bgImg = document.querySelector(".tort-order__img img");
       imgUrl = bgImg.getAttribute('src');
       bg.innerHTML += '<div class="img" style="background-image: url(' + imgUrl + ')">';
    }

/* End */
